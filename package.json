{
  "name": "reactvite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite && npm:watch:tailwind",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:tailwind": "tailwindcss -i ./src/index.css -o ./src/styles.css --minify",
    "watch:tailwind": "tailwindcss -i ./src/index.css -o ./src/styles.css --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@types/react-router-dom": "5.3.3",
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.0",
    "font-awesome": "^4.7.0",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.40.0",
    "react-query": "^3.39.2",
    "react-router-dom": "6.4.5",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "msw": "^0.49.2",
    "prettier": "^2.8.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
